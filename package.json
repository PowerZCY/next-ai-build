{
  "name": "windrun-huaiin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "start": "pnpm -r start",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean",
    "ddaas:dev": "pnpm --filter=@windrun-huaiin/ddaas-website dev",
    "ddaas:build": "pnpm --filter=@windrun-huaiin/ddaas-website build",
    "ddaas:start": "pnpm --filter=@windrun-huaiin/ddaas-website start"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ],
    "overrides": {
      "@types/react": "19.1.2",
      "@types/react-dom": "19.1.3"
    },
    "patchedDependencies": {
      "fumadocs-ui@15.3.3": "patches/fumadocs-ui@15.3.3.patch"
    }
  }
}

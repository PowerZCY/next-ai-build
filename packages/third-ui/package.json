{
  "name": "@windrun-huaiin/third-ui",
  "version": "5.12.2",
  "description": "Third-party integrated UI components for windrun-huaiin projects",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./clerk": {
      "types": "./dist/clerk/index.d.ts",
      "import": "./dist/clerk/index.mjs",
      "require": "./dist/clerk/index.js"
    },
    "./main": {
      "types": "./dist/main/index.d.ts",
      "import": "./dist/main/index.mjs",
      "require": "./dist/main/index.js"
    },
    "./fuma/server": {
      "types": "./dist/fuma/server.d.ts",
      "import": "./dist/fuma/server.mjs",
      "require": "./dist/fuma/server.js"
    },
    "./fuma/mdx": {
      "types": "./dist/fuma/mdx/index.d.ts",
      "import": "./dist/fuma/mdx/index.mjs",
      "require": "./dist/fuma/mdx/index.js"
    },
    "./lib/server": {
      "types": "./dist/lib/server.d.ts",
      "import": "./dist/lib/server.mjs",
      "require": "./dist/lib/server.js"
    },
    "./styles/third-ui.css": "./dist/third-ui.css",
    "./src/*": "./src/*"
  },
  "files": [
    "src",
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:css": "postcss src/styles/third-ui.css -o dist/third-ui.css",
    "build": "tsup && pnpm build:css",
    "build:prod": "tsup && pnpm build:css",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/localizations": "^3.16.0",
    "@clerk/types": "^4.59.0",
    "@clerk/nextjs": "^6.19.4",
    "@windrun-huaiin/base-ui": "workspace:^",
    "fumadocs-core": "15.3.3",
    "fumadocs-mdx": "11.6.3",
    "fumadocs-typescript": "4.0.4",
    "fumadocs-ui": "15.3.3",
    "class-variance-authority": "^0.7.1",
    "katex": "^0.16.22",
    "mermaid": "^11.6.0",
    "react-medium-image-zoom": "^5.2.14",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "next": "15.3.2",
    "next-intl": "^3.26.5",
    "next-themes": "^0.4.6",
    "tailwindcss": "^4.0.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^3.0.0",
    "nprogress": "^0.2.0"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "tsup": "^8.3.5",
    "@types/mdx": "^2.0.13",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.3",
    "@types/node": "^22.0.0",
    "@types/nprogress": "^0.2.3",
    "tailwindcss": "^4.1.7"
  },
  "keywords": [
    "ui",
    "components",
    "react",
    "nextjs",
    "clerk",
    "fumadocs",
    "tailwindcss"
  ],
  "author": "windrun-huaiin",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
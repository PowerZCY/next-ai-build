	"mdx-cards-grop": {
		"prefix": "@mdxcg",
		"body": [
			"<Cards>",
			"  <Card icon={<${1} />} title=\"${2}\" href=\"${3}\" >",
			"    ${4}",
			"  </Card>",
			"</Cards>",
			"",
			"${0}"
		],
		"description": "MDX Cards Group",
		"isFileTemplate": false
	},
	"mdx-card": {
		"prefix": "@mdxscard",
		"body": [
			"<Card icon={<${1} />} title=\"${2}\" href=\"${3}\" >",
			"  ${4}",
			"</Card>",
			"${0}"
		],
		"description": "MDX Single Card",
		"isFileTemplate": false
	},
	"mdx-install-tab": {
		"prefix": "@mdxinstalltab",
		"body": [
			"<Tabs groupId=\"package-manager\" persist items={[\"npm\", \"pnpm\", \"yarn\", \"bun\"]}>",
			"  ```bash tab=\"npm\"",
			"  npm install ${1}",
			"  ```",
			"",
			"  ```bash tab=\"pnpm\"",
			"  pnpm add ${1}",
			"  ```",
			"",
			"  ```bash tab=\"yarn\"",
			"  yarn add ${1}",
			"  ```",
			"",
			"  ```bash tab=\"bun\"",
			"  bun add ${1}",
			"  ```",
			"</Tabs>",
			"",
			"${0}"
		],
		"description": "MDX Install Tab",
		"isFileTemplate": false
	},
  "mdx-tabs": {
		"prefix": "@mdxtabs",
		"body": [
			"<Tabs groupId=\"${2}\" persist items={[\"${1}\"]}>",
			"  ```${3} title=\"${1}\" tab=\"${1}\" ",
			"  ${4}",
			"  ```",
			"</Tabs>",
			"${0}"
		],
		"description": "MDX Tabs",
		"isFileTemplate": false
	},
  "mdx-tabs-item": {
		"prefix": "@mdxstab",
		"body": [
			"```${2} title=\"${1}\" tab=\"${1}\" ",
			"  ${3}",
			"```",
			"${0}"
		],
		"description": "MDX Tabs Item",
		"isFileTemplate": false
	},
  "mdx-include-tabs": {
		"prefix": "@mdxitabs",
		"body": [
			"<Tabs groupId=\"${2}\" items={[\"${1}\"]}>",
			"  <Tab value=\"${1}\">",
      "    <include cwd lang=\"${1}\" meta='title=\"${3}.${1}\"'>",
      "      ${4}/${3}.${1}",
      "    </include>",
      "  </Tab>",
      "</Tabs>",
			"${0}"
		],
		"description": "MDX Tabs",
		"isFileTemplate": false
	},
  "mdx-include-tab-item": {
		"prefix": "@mdxistab",
		"body": [
			"<Tab value=\"${1}\">",
      "  <include cwd lang=\"${1}\" meta='title=\"${2}.${1}\"'>",
      "    ${3}/${2}.${1}",
      "  </include>",
      "</Tab>",
			"${0}"
		],
		"description": "MDX Tabs",
		"isFileTemplate": false
	},
  "mdx-include-file": {
		"prefix": "@mdxif",
		"body": [
      "<include cwd lang=\"${1}\" meta='title=\"${2}.${1}\"'>",
      "  ${3}/${2}.${1}",
      "</include>",
			"${0}"
		],
		"description": "MDX Tabs",
		"isFileTemplate": false
	},
